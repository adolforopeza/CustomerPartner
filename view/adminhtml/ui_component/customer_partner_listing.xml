<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">customer_partner_listing.partner_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>customer_partner_columns</spinner>
		<deps>
			<dep>customer_partner_listing.partner_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">New Partner</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="partner_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">entity_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Aoropeza_CustomerPartner::customer_partner</aclResource>
		<dataProvider name="partner_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>entity_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="customer_partner_columns">
		<column name="entity_id">
			<settings>
                <draggable>false</draggable>
                <sortable>true</sortable>
                <controlVisibility>true</controlVisibility>
                <visible>true</visible>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
        <column name="name">
            <settings>
                <draggable>false</draggable>
                <sortable>true</sortable>
                <controlVisibility>true</controlVisibility>
                <visible>true</visible>
                <filter>text</filter>
                <label translate="true">NAME</label>
                <editor>
                    <editorType>text</editorType>
                    <validation>
                        <rule name="required-entry" xsi:type="boolean">false</rule>
                    </validation>
                </editor>
            </settings>
        </column>
        <column name="description">
            <settings>
                <draggable>false</draggable>
                <sortable>true</sortable>
                <controlVisibility>true</controlVisibility>
                <visible>true</visible>
                <filter>text</filter>
                <label translate="true">DESCRIPTION</label>
                <editor>
                    <editorType>text</editorType>
                    <validation>
                        <rule name="required-entry" xsi:type="boolean">false</rule>
                    </validation>
                </editor>
            </settings>
        </column>
		<column name="url_key">
			<settings>
                <draggable>false</draggable>
                <sortable>true</sortable>
                <controlVisibility>true</controlVisibility>
                <visible>true</visible>
				<filter>text</filter>
				<label translate="true">URL</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="customer_group_id" component="Aoropeza_CustomerPartner/js/grid/columns/customergroup">
			<settings>
                <draggable>false</draggable>
                <sortable>true</sortable>
                <controlVisibility>true</controlVisibility>
                <visible>true</visible>
				<filter>select</filter>
				<label translate="true">CUSTOMER GROUP</label>
                <options class="Magento\Customer\Ui\Component\Listing\Column\Group\Options"/>
                <dataType>select</dataType>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="is_active" component="Aoropeza_CustomerPartner/js/grid/columns/custom">
			<settings>
                <draggable>false</draggable>
                <sortable>true</sortable>
                <controlVisibility>true</controlVisibility>
                <visible>true</visible>
				<filter>text</filter>
				<label translate="true">ACTIVE</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="created">
			<settings>
                <draggable>false</draggable>
                <sortable>true</sortable>
                <controlVisibility>true</controlVisibility>
                <visible>false</visible>
				<filter>text</filter>
				<label translate="true">CREATED</label>
			</settings>
		</column>
		<column name="updated">
			<settings>
                <draggable>false</draggable>
                <sortable>true</sortable>
                <controlVisibility>true</controlVisibility>
                <visible>false</visible>
				<filter>text</filter>
				<label translate="true">UPDATED</label>
			</settings>
		</column>
        <actionsColumn name="actions" class="Aoropeza\CustomerPartner\Ui\Component\Listing\Column\CustomerpartnerActions">
            <settings>
                <indexField>entity_id</indexField>
                <resizeEnabled>false</resizeEnabled>
                <resizeDefaultWidth>107</resizeDefaultWidth>
            </settings>
        </actionsColumn>
	</columns>
</listing>
